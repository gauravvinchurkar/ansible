{
    "dashboard": {
      "title": "Custom Dashboard",
      "panels": [
        {
          "title": "CPU Usage",
          "type": "graph",
          "targets": [
            {
              "expr": "node_cpu_seconds_total{mode=\"idle\"}"
            }
          ]
        },
        {
          "title": "Memory Usage",
          "type": "graph",
          "targets": [
            {
              "expr": "node_memory_MemFree_bytes"
            }
          ]
        }
      ]
    }
  }